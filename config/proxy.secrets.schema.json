{
    "$schema": "http://json-schema.org/draft-07/schema",
    "$id": "proxy.secrets.schema.json-v0.0.1",
    "type": "object",
    "title": "虚拟校园网秘密信息",
    "properties": {
        "username": {
            "title": "用户名",
            "type": "string",
            "description": "一般与学号一致。"
        },
        "password": {
            "title": "密码",
            "type": "string",
            "description": "[统一身份认证](https://login.bit.edu.cn)的密码。"
        }
    },
    "required": [
        "username",
        "password"
    ]
}