{
    "compilerOptions": {
        "module": "ESNext",
        "moduleResolution": "Node",
        "target": "ESNext",
        "allowSyntheticDefaultImports": true,
        "outDir": "dist"
    },
    "exclude": [
        "node_modules"
    ],
    "typedocOptions": {
        "entryPointStrategy": "expand",
        "entryPoints": [
            "src/"
        ],
        "out": "docs"
    }
}